rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      // allow read, write: if
      //     request.time < timestamp.date(2021, 1, 24);
      allow write, read, update, delete: if request.auth != null && request.auth.token.email == "jocvegar@gmail.com";

    }
  }
}